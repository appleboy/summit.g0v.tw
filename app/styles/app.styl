.noimg
  height: 60px
  padding: 20px 0

.navbar-brand
  padding: 15px 16px
  .noimg
    padding: 18px 0

@media (max-width: 767px)
  .navbar-brand .noimg
    padding: 0

.navbar-fixed-top .navbar-nav li 
  text-align: center
  img
    width: 60px
    height: 60px

@media (min-width: 768px) and (max-width: 992px)
  .navbar-fixed-top .navbar-nav li a
    padding: 15px 7px
    img
      width: 39px

.page-title
  margin-top: 20px
  margin-bottom: 10px

#schedule
  border-left 1px solid
  border-top 1px solid
  display flex
  flex-wrap wrap
  .time, .topic
    border-bottom 1px solid
    border-right 1px solid
    padding 5px
    text-align center
  .time
    flex 0 1 8%
  .topic
    flex 1 1 23%
    &.common
      flex 1 1 92%
    &.row2
      border-bottom 0
  .track
    display none

@media (max-width: 768px)
  #schedule
    flex-direction column
    .time, .topic, .topic.common
      flex 1 1 auto
    .track
      background-color yellow
      display inline-block

span.fui-user
  padding-right: 5px

.session-brief p
  text-indent: 20px 
